{
  "hosting": {
    "public": "public",
    "headers": [{
      "source":"**",
      "headers": [
        {
          "key": "Content-Security-Policy", "value": "default-src *; img-src *; style-src *; script-src *"
        }
      ]
    }],
    "rewrites": [
      {
          "source": "api/**",
          "function": "api"
      },
      {
        "source": "**",
        "function": "app"
      }
    ],
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ]
  },
  "functions": {
    "source": "functions"
  }
}

{
  "hosting": {
    "public": "public",
    "headers": [{
      "source":"**",
      "headers": [
        {"key":"Content-Security-Policy", "value":""}
      ]
    }],
    "rewrites": [
      {
          "source": "api/**",
          "function": "api"
      },
      {
        "source": "**",
        "function": "app"
      }
    ],
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ]
  },
  "functions": {
    "source": "functions"
  }
}

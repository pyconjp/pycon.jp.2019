{
  "name": "pycon.jp.2019",
  "version": "1.0.0",
  "description": "PyCon JP 2019 Web Site",
  "author": "Ikeda Yutao",
  "private": true,
  "scripts": {
    "test": "jest",
    "dev": "nuxt --port 4000",
    "build:dev": "DEPLOY_ENV=DEV nuxt generate && rm -rf functions/dist/* && mkdir -p functions/dist/dev-2019 && cp -r dist/* functions/dist/dev-2019/",
    "build:branch": "nuxt generate && rm -rf functions/dist/* && mkdir -p functions/dist && cp -r dist/* functions/dist/",
    "build:prd": "DEPLOY_ENV=PRD nuxt generate && rm -rf functions/dist/* && mkdir -p functions/dist/2019 && cp -r dist/* functions/dist/2019/",
    "start": "nuxt start",
    "generate": "nuxt generate",
    "convert-sponsors-csv": "yarn run -s csv2json data/sponsors.csv assets",
    "convert-sessions-csv": "yarn run -s csv2json data/sessions.csv assets",
    "convert-staffs-csv": "yarn run -s csv2json data/staffs.csv assets "
  },
  "dependencies": {
    "@nuxtjs/axios": "^5.3.6",
    "@nuxtjs/vuetify": "0.5.5",
    "core-js": "3",
    "csv2json": "./scripts/csv2json",
    "lodash": "^4.17.19",
    "marked": "^0.7.0",
    "moment": "^2.24.0",
    "nuxt": "^2.0.0",
    "nuxt-i18n": "^5.12.7",
    "vue-i18n": "^8.11.2",
    "vue-sticky-directive": "^0.0.10",
    "vue2-google-maps": "^0.10.6"
  },
  "devDependencies": {
    "@babel/plugin-syntax-dynamic-import": "^7.2.0",
    "@babel/plugin-transform-runtime": "^7.5.5",
    "@babel/preset-env": "^7.5.5",
    "@babel/runtime": "^7.5.5",
    "@fortawesome/fontawesome-free": "^5.9.0",
    "@nuxtjs/dotenv": "^1.3.0",
    "@vue/test-utils": "^1.0.0-beta.27",
    "babel-core": "^7.0.0-0",
    "babel-jest": "^24.1.0",
    "babel-loader": "^8.0.6",
    "babel-polyfill": "^6.26.0",
    "eslint-config-prettier": "^4.1.0",
    "eslint-plugin-prettier": "^3.0.1",
    "eslint-utils": "^1.4.2",
    "jest": "^24.1.0",
    "node-sass": "^4.12.0",
    "nodemon": "^1.18.9",
    "postcss-preset-env": "^6.7.0",
    "prettier": "^1.16.4",
    "pug": "^2.0.4",
    "pug-plain-loader": "^1.0.0",
    "push-dir": "^0.4.1",
    "regenerator-runtime": "^0.13.3",
    "sass-loader": "^7.1.0",
    "stylus": "^0.54.5",
    "stylus-loader": "^3.0.2",
    "vue-jest": "^3.0.3"
  }
}
